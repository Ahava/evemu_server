<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="XMLPKT Generator"
	Version="9,00"
	>
	<Rules>
		<CustomBuildRule
			Name="XMLPkt"
			DisplayName="XML Packet Generator"
			CommandLine="&quot;$(SolutionDir)$(ConfigurationName)/xmlpktgen/XMLPktGen.exe&quot; [inputs]"
			Outputs="$(InputName).h"
			AdditionalDependencies="&quot;$(SolutionDir)$(ConfigurationName)/xmlpktgen/XMLPktGen.exe&quot;"
			FileExtensions="*.xmlp"
			ExecutionDescription="Generating packet processors from XML: $(InputName)"
			ShowOnlyRuleProperties="false"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
